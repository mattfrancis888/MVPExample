public class MainPresenter implements MovieAPIAndRecyclerViewContract.Presenter {
    APIServiceModel apiServiceModel;
    MovieAPIAndRecyclerViewContract.View view;
    MovieAPIPresenter.MovieAPIPresenterListener movieAPIPresenterListener;

    public MainPresenter(APIServiceModel apiServiceModel,
                         MovieAPIAndRecyclerViewContract.View view,
                         MovieAPIPresenter.MovieAPIPresenterListener movieAPIPresenterListener){
        this.apiServiceModel = apiServiceModel;
        this.view = view;
        this.movieAPIPresenterListener = movieAPIPresenterListener;
    }


    @Override
    public void setRecyclerViewData() {
        new MovieAPIPresenter(apiServiceModel, movieAPIPresenterListener).
                getMovies(view.getAPIKey(), "en-US", "popularity.desc", "1");

        
    }

}

